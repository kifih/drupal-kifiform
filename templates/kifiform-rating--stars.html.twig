<span class="kifiform-rating-item" property="schema:aggregateRating" typeof="schema:AggregateRating">
  <span class="rating-bar" aria-label="{% trans %}{{ stars }} out of 5 stars{% endtrans %}">
    <span class="rating-fill" style="width: {{ stars * 20 }}%" data-bind="rating"></span>
  </span>

  <meta property="schema:worstRating" content="0"/>
  <meta property="schema:bestRating" content="100"/>
  <meta property="schema:ratingValue" content="{{ rating }}"/>

  {% if display_votes %}
    <meta property="schema:ratingCount" content="{{ votes }}">
    <span class="rating-votes" data-bind="votes">{% trans %}{{ votes }} votes{% endtrans %}</span>
  {% endif %}
</span>
